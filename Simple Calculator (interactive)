# run: python calc.py
def calc():
    a=float(input("a: ")); op=input("op (+ - * / ^): "); b=float(input("b: "))
    if op=='+': print(a+b)
    elif op=='-': print(a-b)
    elif op=='*': print(a*b)
    elif op=='/': print(a/b if b!=0 else "Divide by 0")
    elif op=='^': print(a**b)
    else: print("Unknown op")
if __name__=="__main__": calc()
